<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
  <head>
    <title>Layout title</title>

    <!-- Panels -->
    <link rel="panel" rev="content" target="content" />

    <!-- Head elements -->
    <link  rel="tile" target="plone-htmlhead" href="./@@plone.app.standardtiles.viewletmanager?manager=plone.htmlhead" />
    <link  rel="tile" target="plone-htmlhead-links" href="./@@plone.app.standardtiles.viewletmanager?manager=plone.htmlhead.links" />

    <!-- Viewlet managers -->
    <link  rel="tile" target="plone-portaltop" href="./@@plone.app.standardtiles.viewletmanager?manager=plone.portaltop" />
    <link  rel="tile" target="viewlet-above-content" href="./@@plone.app.standardtiles.viewletmanager?manager=plone.abovecontent" />
    <link  rel="tile" target="edit-bar" href="./@@plone.app.standardtiles.viewletmanager?manager=plone.contentviews" />
    <link  rel="tile" target="viewlet-below-content" href="./@@plone.app.standardtiles.viewletmanager?manager=plone.belowcontent" />
    <link  rel="tile" target="plone-portalfooter" href="./@@plone.app.standardtiles.viewletmanager?manager=plone.portalfooter" />

    <!-- Head viewlet managers -->
    <div id="plone-htmlhead"/>
    <div id="plone-htmlhead-links"/>
  </head>
  <body>
    <div id="visual-portal-wrapper">

      <div id="portal-top" class="row">
        <div class="cell width-full position-0">
          <div id="plone-portaltop"/>
        </div>
      </div>

      <div id="portal-columns" class="row">

        <div id="portal-column-content" class="cell width-full position-0">
          <div id="viewlet-above-content"/>
          <div class="documentEditable">
             <div id="edit-bar"/>

<!--
             <div metal:use-macro="context/global_statusmessage/macros/portal_message">
               Status message
             </div>
-->

            <div id="content"/>
          </div>
          <div id="viewlet-below-content"/>

        </div>
      </div>

      <div id="plone-portalfooter"/>

    </div>
  </body>
</html>
